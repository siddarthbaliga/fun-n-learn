first=str(raw_input("Enter the 1st alphabet of your name"))
second=str(raw_input("Enter the 2nd alphabet of your name"))
third=str(raw_input("Enter the 3rd alphabet of your name"))
fourth=str(raw_input("Enter the 4th alphabet of your name"))
fifth=str(raw_input("Enter the 5th alphabet of your name"))
print(first+second+third+fourth+fifth)
print(second+"!!!"+fourth)
print(third+"!!!"+third)
print(fourth+"!!!"+second)
print(fifth+fourth+third+second+first)
